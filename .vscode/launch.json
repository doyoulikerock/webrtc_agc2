{
	// IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
	// 기존 특성에 대한 설명을 보려면 가리킵니다.
	// 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
	"version": "0.2.0",
	"configurations": [
		{
            "name": "CMake Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/test_my",
            "args": [
				"/root/agc2.input.wav",
				"/root/agc2.output.wav"
			],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
				{
					"name": "LD_LIBRARY_PATH",
					"value" : "${workspaceFolder}/build/lib"
					//"value": "${workspaceFolder}/../../external/packages/lib"
				},
			],
            "externalConsole": false,
            "MIMode": "gdb"
        }


	]
}